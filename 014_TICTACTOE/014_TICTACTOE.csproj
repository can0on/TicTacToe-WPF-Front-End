<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>_014_TICTACTOE</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>tictactoe_icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\.png" />
    <None Remove="Images\White\circle.png" />
    <None Remove="Images\White\x_sign.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="tictactoe_icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\White\circle.png" />
    <Resource Include="Images\White\x_sign.png" />
  </ItemGroup>

</Project>
